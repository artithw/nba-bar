{
  "name": "nba-bar",
  "description": "The best NBA menubar App",
  "license": "MIT",
  "author": "xxhomey19 <xxhomey19@gmail.com> (https://github.com/xxhomey19)",
  "repository": "https://github.com/xxhomey19/nba-bar",
  "bugs": {
    "url": "https://github.com/xxhomey19/nba-bar/issues"
  },
  "version": "1.0.1",
  "main": "main.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "precommit": "lint-staged",
    "dev": "cross-env NODE_ENV=development concurrently -k './node_modules/.bin/webpack-dev-server --config ./webpack.config.development.js' 'npm start'",
    "dist": "npm run build && rimraf release && build --mac --win",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint src",
    "lint:css": "stylelint 'src/**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "pack": "electron-builder --dir",
    "start": "electron .",
    "test": "npm run testonly",
    "testonly": "cross-env NODE_ENV=test jest",
    "testonly:cov": "cross-env NODE_ENV=test jest --coverage --runInBand --forceExit",
    "testonly:watch": "cross-env NODE_ENV=test jest --watch"
  },
  "dependencies": {
    "antd-mobile": "^2.1.8",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.4",
    "date-fns": "^1.29.0",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.3",
    "electron-updater": "^2.21.4",
    "fix-path": "^2.1.0",
    "menubar": "^5.2.3",
    "moment-timezone": "^0.5.15",
    "nba-color": "^1.3.0",
    "nba-stats-client": "^0.0.4",
    "nba.js": "^0.6.1",
    "p-map": "^1.2.0",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.0.1",
    "react-md-spinner": "^0.2.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-sticky-table": "^2.0.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.6",
    "webpack": "^4.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-ramda": "^1.6.1",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "devtron": "^1.4.0",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "json-loader": "^0.5.7",
    "lint-staged": "^7.0.4",
    "node-noop": "^1.0.0",
    "prettier": "^1.12.1",
    "prettier-package-json": "^1.6.0",
    "react-test-renderer": "^16.3.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.3",
    "stylelint": "9.1.3",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-config-yoctol": "^0.8.0",
    "stylelint-processor-styled-components": "^1.3.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "keywords": [
    "App",
    "Bar",
    "Electron",
    "Menubar",
    "NBA"
  ],
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "build": {
    "appId": "com.nba.bar",
    "productName": "NBA Bar",
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "files": [
      "dist/",
      "resources/",
      "index.html",
      "main.js",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "publish": {
      "provider": "github",
      "repo": "nba-bar",
      "owner": "xxhomey19"
    },
    "mac": {
      "category": "public.app-category.sports",
      "icon": "resources/icon.png",
      "artifactName": "nba-bar-${version}-mac.${ext}"
    },
    "win": {
      "icon": "resources/icon.png",
      "artifactName": "nba-bar-${version}-win.${ext}"
    }
  },
  "jest": {
    "globals": {
      "window": true
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "npm run lint:css",
      "git add"
    ]
  },
  "productName": "NBA Bar"
}
