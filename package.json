{
  "name": "nba-menubar",
  "description": "NBA Menubar app",
  "author": "xxhomey19",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "build": "webpack --config webpack.config.production.js --progress",
    "precommit": "lint-staged",
    "dev": "NODE_ENV=development concurrently -k './node_modules/.bin/webpack-dev-server --config ./webpack.config.development.js' 'npm start'",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint src",
    "lint:css": "stylelint './src/**/*.js, !**/__tests__/**'",
    "lint:fix": "npm run lint -- --fix",
    "package": "npm run build && electron-builder",
    "start": "electron .",
    "test": "npm run testonly",
    "testonly": "cross-env NODE_ENV=test jest",
    "testonly:cov": "NODE_ENV=test jest --coverage --runInBand --forceExit",
    "testonly:watch": "NODE_ENV=test jest --watch"
  },
  "dependencies": {
    "antd-mobile": "^2.1.8",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "date-fns": "^1.29.0",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.3",
    "fix-path": "^2.1.0",
    "menubar": "^5.2.3",
    "moment-timezone": "^0.5.14",
    "nba-color": "^1.3.0",
    "nba-stats-client": "^0.0.4",
    "nba.js": "^0.6.0",
    "p-map": "^1.2.0",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-md-spinner": "^0.2.5",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-sticky-table": "^2.0.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.3",
    "webpack": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.6.7",
    "babel-plugin-ramda": "^1.5.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "devtron": "^1.4.0",
    "electron": "^1.8.4",
    "electron-builder": "^20.5.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "json-loader": "^0.5.7",
    "lint-staged": "^7.0.0",
    "node-noop": "^1.0.0",
    "prettier": "^1.11.1",
    "prettier-package-json": "^1.5.1",
    "react-test-renderer": "^16.3.1",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-config-yoctol": "^0.7.0",
    "stylelint-processor-styled-components": "^1.3.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^1.0.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.1"
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "build": {
    "appId": "org.epilande.yarems",
    "productName": "NBA Menubar",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "resources/",
      "index.html",
      "main.js",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "resources/app.icns"
    }
  },
  "jest": {
    "globals": {
      "widow": true
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "npm run lint:css",
      "git add"
    ]
  }
}
